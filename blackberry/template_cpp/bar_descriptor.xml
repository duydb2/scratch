<?xml version="1.0" encoding="utf-8" standalone="no"?>
<qnx>
   <id>com.example.template_qml</id>
   <name>CPP Template</name>
   <versionNumber>1.0.0</versionNumber>
   <description>CPP Template</description>

   <initialWindow>
      <systemChrome>none</systemChrome>
      <transparent>false</transparent>
      <autoOrients>true</autoOrients>
      <aspectRatio>landscape</aspectRatio>
   </initialWindow>

   <env var="QT_PLUGIN_PATH" value="app/native/plugins"/>
   <env var="LD_LIBRARY_PATH" value="app/native/lib"/>

   <arg>-platform</arg>
   <arg>blackberry</arg>
   <author>canurabus</author>

   <action system="true">run_native</action>

   <category>core.media</category>

   <asset entry="true" path="template_cpp" type="Qnx/Elf">template_cpp</asset>
   <asset path="deploy/lib">lib</asset>
   <asset path="deploy/plugins">plugins</asset>
</qnx>
